
	<table summary="DmItem form fields">
      <tr>
        <th>ID</th>
        <td><%= f.text_field(:auto_id) %></td>
      </tr>
      <tr>
        <th>ArchivesSpace RefID</th>
        <td><%= f.text_field(:refid) %></td>
      </tr>
      <tr>
        <th>Parent Component</th>
        <td><%= f.text_field(:display_title) %></td>
        <th>Parent Resource</th>
        <td><%= f.text_field(:resource) %></td>
      </tr>
      <tr>
        <th>Format</th>
        <td><%= f.text_field(:format) %></td>
      </tr>
      <tr>
        <th>Transfer Status</th>
        <td><%= f.text_field(:status) %></td>
        <th>Transfer Method</th>
        <td><%= f.text_field(:method) %></td>
      </tr>
      <tr>
        <th>Transfer Date</th>
        <td><%= f.date_field(:transfer_date) %></td>
      </tr>
      <tr>
        <th>Disposition</th>
        <td><%= f.text_field(:disposition) %></td>
      </tr>
      <tr>
        <th>Notes</th>
        <td><%= f.text_area(:notes, :size => "40x5") %></td>
      </tr>
      
    </table>