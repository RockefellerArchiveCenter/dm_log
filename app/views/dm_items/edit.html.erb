<% @page_title = "Edit" %>
<%= link_to("<< Back to List", {:action => 'index'}, :class => 'back-link') %>

<div class="dm_items edit">
  <h2>Update DmItem</h2>

  <%= form_for(:dm_item, :url => {:action => 'update', :id => @dm_item.id}) do |f| %>

    <%= render(:partial => "form", :locals => {:f => f}) %>

    <div class="form-buttons">
      <%= submit_tag("Update DmItem") %>
    </div>

  <% end %>
</div>
